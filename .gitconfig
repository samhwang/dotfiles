	name = Sam Huynh
	email = samhwang21121994@gmail.com
[alias]
  	lol = log --graph --decorate --pretty=oneline --abbrev-commit
  	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[color]
	# Update on 13/2/2019: Change the color for branch, diff and status to the
	# three blocks below.
  	# diff = auto
  	# status = auto
  	# branch = auto
  	interactive = auto
  	ui = true
  	pager = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
  	editor = code --wait
	excludesfile = ~/.gitexclude
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
